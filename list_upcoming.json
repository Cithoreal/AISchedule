{
    "name": "list_upcoming",
    "description": "List upcoming events and tasks",
    "parameters": {
        "type": "object",
        "properties": {
            "upcoming_list": {
                "type": "object",
                "properties": {
                    "CALENDAR": {
                        "type": "string",
                        "description": "Calendar to search in"
                    },
                    "DTSTART": {
                        "type": "string",
                        "description": "Start date to search from"
                    },
                    "DTEND": {
                        "type": "string",
                        "description": "End date to search to"
                    },
                    "LIST_EVENTS": {
                        "type": "boolean",
                        "description": "Include events"
                    },
                    "LIST_TASKS": {
                        "type": "boolean",
                        "description": "Include tasks"
                    },
                    "UNSCHEDULED_TASKS": {
                        "type": "boolean",
                        "description": "Include unscheduled tasks"
                    }
                },
                "required": [
                    "CALENDAR",
                    "DTSTART",
                    "DTEND",
                    "UNSCHEDULED_TASKS"
                ]
            }
        },
        "required": [
            "upcoming_list"
        ]
    }
}